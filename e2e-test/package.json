{
  "name": "sdk-e2e-test",
  "version": "0.6.0",
  "private": true,
  "description": "E2E testing application for the SDK's features",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test --config=playwright.config.ts",
    "test:ui": "playwright test --config=playwright.config.ts --ui",
    "test:headed": "playwright test --config=playwright.config.ts --headed",
    "test:debug": "playwright test --config=playwright.config.ts --debug",
    "test:docker": "PW_TEST_CONNECT_WS_ENDPOINT=ws://127.0.0.1:3100/ playwright test --config=playwright.config.ts",
    "playwright:docker": "./scripts/playwright-docker.sh 3100",
    "setup": "rm -f ohara-ai-data/* || true && npm run deploy-devworld-token && npm run deploy-factories && npm run fund-controller",
    "deploy-factories": "./scripts/deploy-factories.sh",
    "deploy-devworld-token": "./scripts/deploy-devworld-token.sh",
    "fund-controller": "./scripts/fund-controller.sh",
    "populate": "tsx scripts/populate-contracts.ts"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^1.1.2",
    "@ohara-ai/sdk": "file:../sdk",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.62.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "^15.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.5.5",
    "viem": "^2.21.45",
    "wagmi": "~2.19.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.9.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
